@Article{DeLuisNovakCufarEtAl2009,
  author =   {De Luis, M. and Novak, K. and Cufar, K. and Raventos, J.},
  title =    {Size mediated climate-growth relationships in Pinus halepensis and Pinus pinea},
  journal =  {Trees-Structure and Function},
  year =     {2009},
  volume =   {23},
  number =   {5},
  pages =    {1065-1073},
  note =     {ISI Document Delivery No.: 488XR Times Cited: 17 Cited Reference Count: 30 De Luis, Martin Novak, Klemen Cufar, Katarina Raventos, Jose Spanish Ministry of Education and Science [CGL2005-04270, CGL2007-65315-C03-02, CGL2008-05112C02-01] This study was supported by Spanish Ministry of Education and Science co-funded by FEDER program (projects: CGL2005-04270, CGL2007-65315-C03-02 and CGL2008-05112C02-01). We also thank Elaine Rowe for her work in improving the English of this manuscript. Springer New york},
  abstract = {Functional processes in trees undergo changes as the tree size increases, which may affect the response of trees to environmental factors. We tested tree-ring response to climate in four groups of trees, including large and small Pinus halepensis and Pinus pinea trees using cluster, principal component (PC) and dendroclimatological analysis. The trees were of the same age and growing on a plantation in the semiarid coastal area of southern Spain. Cluster and PC analyses showed a clear separation into four groups of trees. Autocorrelation and mean sensitivity showed significant differences between the two size classes. PCA recognised four representative principal components where PC1 represented the tree-ring-climate variability common to both species and sizes, whereas PC2, PC3 and PC4 represented a species-specific and size-dependent response of trees to climate. The differences between the two size classes were greater than those between the two species. The results suggest that future tree-ring studies should include trees stratified by size. Only this would make it possible to produce unbiased predictions on the consequences of climate change and to devise suitable mitigation strategies for preserving Mediterranean forest ecosystems.},
  doi =      {10.1007/s00468-009-0349-5},
  issn =     {0931-1890},
  keywords = {Tree size Dendrochronology Tree-rings Aleppo pine Stone pine Climate-growth response Mediterranean intraannual density-fluctuations tree-ring chronologies global change responses signals dry sylvestris spain width young},
  type =     {Journal Article},
  url =      {<Go to ISI>://WOS:000269384200018}
}

