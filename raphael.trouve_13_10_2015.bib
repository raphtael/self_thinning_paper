@Article{Dhote1999,
  author =   {Dhôte, J. F.},
  title =    {Competition based on social rank in sessile oak and beech. Competition entre classes sociales chez le chene sessile et le hetre},
  journal =  {Revue Forestière Française},
  year =     {1999},
  volume =   {51},
  number =   {2},
  pages =    {309-325},
  note =     {Cited Reference Count: 18 ref. Issue special. Fonctionnement des arbres et ecosystemes forestiers. Avancees recentes et consequences sylvicoles.},
  abstract = {Even-aged single-species stands of beech ( Fagus sylvatica) or sessile oak ( Quercus petraea) in France are frequently in conflict from the standpoint of dynamics (stand growth rate), the factors that determine wood quality (the effect of fast/slow growth regimes) and the effect of thinning (the capability of remaining trees to benefit from thinning operations). The author deals with the last point by analysing the competition between social ranks of trees, in particular, how thinning regulates competition, and what its effect is on differential growth of tree social classes. The more localized aspects of competition (the stimulating effect of localized thinning, or trimming, for the benefit of final crop trees) are not considered. The analysis was performed on networks of permanent plots established between 1880 and 1935. The analyses showed up features in common between the two species studied, that are appropriately referred to as 'social' features: both tree species form highly structured systems in which individuals which rank highest socially are those that grow the fastest. A simplified model is used to more accurately quantify the relationship between social rank and growth rate. However, there were notable differences between the two species also: for beech, surplus growth afforded by thinning was all the more pronounced the higher the tree's initial status was. For oak, the benefits of thinning were uniform whatever the trees' social status, at least in terms of basal area. These differences in growth behaviour and response to thinning are discussed with reference to competition for light and soil resources, particularly water.},
  keywords = {KK100Forests and Forest Trees (Biology and Ecology) KK110Silviculture and Forest Management KK120Forest Mensuration and Management (Discontinued March 2000) KK500Forest Products and Industries (General) ZZ331Plant Ecology basal area biological competition ecosystems effects forest ecology forests growth increment stand characteristics stand development thinning France Fagus Fagus sylvatica Quercus Quercus petraea Fagaceae Fagales dicotyledons angiosperms Spermatophyta plants eukaryotes Quercus Fagus Western Europe Europe Mediterranean Region Developed Countries European Union Countries OECD Countries},
  type =     {Journal Article},
  url =      {<Go to ISI>://CABI:19990610766}
}

@Article{DhoteDupoueyBerges2000,
  author =   {Dhôte, J. F. and Dupouey, J. L. and Berges, L.},
  title =    {Long term changes observed in the productivity of French forests Modifications a long terme, deja constatees, de la productivite des forets francaises},
  journal =  {Revue Forestiere Francaise},
  year =     {2000},
  volume =   {52},
  number =   {Special},
  pages =    {37-48},
  note =     {Cited Reference Count: 34 ref.},
  abstract = {Several recent European studies have shown that tree growth and stand yield have strongly increased over the past 150 years. In this article, we present a critical review of the methods used: retrospective analyses of radial growth (dendroecology) or height growth (stem analyses); continuous observation of stand yield using permanent plot networks. The advantages and drawbacks of each method are compared, in the light of studies on the effect of environmental changes (CO 2, nitrogen inputs, climate) on French forest dynamics. The first accurate diagnoses at stand level are beginning to be available, and indicate that today's stand productivity is 50% higher than at the beginning of this century. Major research areas now are: (1) to generalize the observations to other species and regions, especially at the stand-level; (2) to evaluate the concomitant changes in wood quality; (3) to better understand the role of environmental changes; and (4) to evaluate the associated ecological risks. In any case, we believe that forest managers should take this accelerated growth into consideration (shorter rotations, increased wood supply, intensified regeneration, forest contribution to policies to mitigate greenhouse effects, etc.).},
  issn =     {0035-2829},
  keywords = {EE112Forestry Economics (NEW March 2000) KK100Forests and Forest Trees (Biology and Ecology) KK110Silviculture and Forest Management KK500Forest Products and Industries (General) KK510Wood Properties, Damage and Preservation PP500Meteorology and Climate PP600Pollution and Degradation PP720Biological Resources (Plant) ZZ331Plant Ecology 124-38-9 7727-37-9 air pollution carbon dioxide climatic change forest policy forests global warming greenhouse effect growth methodology natural regeneration nitrogen pollution control productivity reviews short rotation forestry silvicultural systems stand characteristics techniques timber supply yields France Western Europe Europe Mediterranean Region Developed Countries European Union Countries OECD Countries},
  type =     {Journal Article},
