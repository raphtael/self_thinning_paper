@Article{FontesBontempsBugmannEtAl2010,
  author =   {Fontes, L. and Bontemps, J. D. and Bugmann, H. and Van Oijen, M. and Gracia, C. and Kramer, K. and Lindner, M. and Rotzer, T. and Skovsgaard, J. P.},
  title =    {Models for supporting forest management in a changing environment},
  journal =  {Forest Systems},
  year =     {2010},
  volume =   {19},
  pages =    {8-29},
  note =     {ISI Document Delivery No.: 725YG Times Cited: 2 Cited Reference Count: 121 Fontes, L. Bontemps, J. -D. Bugmann, H. Van Oijen, M. Gracia, C. Kramer, K. Lindner, M. Roetzer, T. Skovsgaard, J. P. Sustainable Forest Management; MOTIVE[226544]; Interreg NWE ForeStClim; SFB[607]; MODNET[PTDC/AGR-CFL/69733/2006] This review paper was accomplished under COST ACTION F P0603 "Forest Models for Research and Decision Support in Sustainable Forest Management" which funded the meetings of the group. The authors gratefully acknowledge Dr Joe Landsberg for helpful comments on the manuscript and several colleagues for their contribution characterizing PBMs, namely Professor Annikki Makela (PipeQual model), Professor Francois Gillet (WoodPaM model), Dr Gabrielle Deckmyn (ANAFORE model), Dr. Georgios Xenak:is (3-PGN model), Dr Eric Dufrene (Castanea model), Dr Annabel Porte (Graeco model), Dr Hans Verbeeck (FORUG model), Dr Heike Lischke (TreeMig and MEPHYSTO models), Professor Heli Peltola and Professor Seppo Kellomaki (FinnFor model), Dr Joao Palma (Yield-SAFE model), Dr Katarina Merganiaova (BIOME-BGC model), Professor Manfred Lexer (PICUS model) and Dr Petra Lasch (4C model). Writing the manuscript had, for several authors, the funding support from the following projects: MOTIVE (FP7 contract no. 226544); Interreg NWE ForeStClim and SFB 607; and MODNET (PTDC/AGR-CFL/69733/2006). Inst nacional investigacion technologia agraria alimentaria Madrid Si},
  abstract = {Forests are experiencing an environment that changes much faster than during the past several hundred years. In addition, the abiotic factors determining forest dynamics vary depending on its location. Forest modeling thus faces the new challenge of supporting forest management in the context of environmental change. This review focuses on three types of models that are used in forest management: empirical (EM), process-based (PBM) and hybrid models. Recent approaches may lead to the applicability of empirical models under changing environmental conditions, such as (i) the dynamic state-space approach, or (ii) the development of productivity-environment relationships. Twenty-five process-based models in use in Europe were analyzed in terms of their structure, inputs and outputs having in mind a forest management perspective. Two paths for hybrid modeling were distinguished: (i) coupling of EMs and PBMs by developing signal-transfer environment-productivity functions; (ii) hybrid models with causal structure including both empirical and mechanistic components. Several gaps of knowledge were identified for the three types of models reviewed. The strengths and weaknesses of the three model types differ and all are likely to remain in use. There is a trade-off between how little data the models need for calibration and simulation purposes, and the variety of input-output relationships that they can quantify. PBMs are the most versatile, with a wide range of environmental conditions and output variables they can account for. However, PBMs require more data making them less applicable whenever data for calibration are scarce. EMs, on the other hand, are easier to run as they require much less prior information, but the aggregated representation of environmental effects makes them less reliable in the context of environmental changes. The different disadvantages of PBMs and EMs suggest that hybrid models may be a good compromise, but a more extensive testing of these models in practice is required.},
  issn =     {2171-5068},
  keywords = {climate change empirical models process-based models mixed models fagus-sylvatica l. of-the-art general quantitative theory central-european forests state-space approach growth-model tree growth ecosystem management climate-change site index},
  type =     {Journal Article},
  url =      {<Go to ISI>://WOS:000287682200002}
}

