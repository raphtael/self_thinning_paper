@Article{Pretzsch2014,
  author =   {Pretzsch, Hans},
  title =    {Canopy space filling and tree crown morphology in mixed-species stands compared with monocultures},
  journal =  {Forest Ecology and Management},
  year =     {2014},
  number =   {0},
  abstract = {Abstract Mixed-species forest stands are well explored in their favourable ecological, economical, and socio-economical functions and services compared with pure stands, but still poorly understood in their structure and functioning. Canopy structure and tree morphology affect the environmental conditions within the stand, the tree growth, and by this most forest functions and services. Here, I review how canopy structure and crown morphology in mixed stands can differ from pure stands and how this depends on the selection of tree species and interactions between them. The focus is on the macrostructure of canopy and crowns derived from the trees’ positions, their convex crown hulls, and their space filling with branches. In mixed canopies the sum of the crown projection area, but not the ground coverage by crowns, mostly exceeds pure stands due to multiple crown overlaps. The interspecific differences in crown shape and allometric scaling cause a ‘selection effect’ when complementary species are combined. In interspecific environment furthermore ‘true mixing effects’ like intraspecific shifts in size, shape, and inner space filling of crowns may occur. The much denser and more plastic canopy space filling in mixed stands may increase light interception, stand density, productivity, and growth resilience to disturbances. I discuss the relevance of interspecific interactions for forest management, model building, and theory development and draw perspectives of further research into stand canopy and crown structure.},
  doi =      {http://dx.doi.org/10.1016/j.foreco.2014.04.027},
  issn =     {0378-1127},
  keywords = {Light interception Interspecific competition Intraspecific competition Overyielding Species selection effect True mixing effect},
  type =     {Journal Article},
  url =      {http://www.sciencedirect.com/science/article/pii/S0378112714002667}
}

@Article{PretzschBiber2005,
  author =   {Pretzsch, H. and Biber, P.},
  title =    {A re-evaluation of Reineke's rule and stand density index},
  journal =  {Forest Science},
  year =     {2005},
  volume =   {51},
  number =   {4},
  pages =    {304-320},
  note =     {ISI Document Delivery No.: 952ME Times Cited: 35 Cited Reference Count: 60 Pretzsch, H Biber, P Soc amer foresters Bethesda},
  abstract = {The self-thinning rule established by Reineke in 1933, N proportional to d(-1.605) (N, d = number of stems and quadratic mean diameter, respectively) assumes the same allometric relationship between size and density for a wide spectrum of species under self-thinning conditions. We re-evaluate this rule based on 28 fully stocked pure stands of common beech (Fagus sylvatica L.), Norway spruce (Picea abies [L.] Karst.), Scots pine (Pinus sylvestris L.), and common oak (Quercus petraea [Mattuschka] Liebl.) in Germany that date back to the year 1870. OLS regression of the model In(N) = a + b (.) ln(d) results in b values of -1.789 for common beech, -1.664 for Norway spruce, -1.593 for Scots pine, and -1.424 for common oak. The allometric coefficient for common beech differs significantly from the other species. There is also a significant difference between the b values of Norway spruce and common oak. Except for Scots pine, the above allometric coefficients deviate significantly (common beech) and close-to-significantly (Norway spruce, common oak) from the coefficient -1.605 postulated by Reineke in 1933. To cover the species-specific oscillation of mortality rates, we additionally calculate the self-thinning coefficient for each survey period. Ecological implications of the species-specific scaling exponents are stressed and consequences regarding tools for regulating stand density are discussed.},
  issn =     {0015-749X},
  keywords = {allometry self-tolerance space sequestration self-thinning Fagus sylvatica L. Picea abies (L.) individual tree mortality self-thinning rule -3/2 power rule law management fir},
  type =     {Journal Article},
  url =      {<Go to ISI>://WOS:000231006800004}
}

@Article{PretzschBiber2010,
  author =   {Pretzsch, H. and Biber, P.},
  title =    {Size-symmetric versus size-asymmetric competition and growth partitioning among trees in forest stands along an ecological gradient in central Europe},
  journal =  {Canadian Journal of Forest Research-Revue Canadienne De Recherche Forestiere},
  year =     {2010},
  volume =   {40},
  number =   {2},
  pages =    {370-384},
  note =     {ISI Document Delivery No.: 572HU Times Cited: 5 Cited Reference Count: 45 Pretzsch, Hans Biber, Peter Deutsche Forschungsgemeinschaft (DFG)[SFB 607]; Bavarian State Ministry for Nutrition, Agriculture, and Forestry[W 07] The authors wish to thank the Deutsche Forschungsgemeinschaft (DFG) for providing funds for forest growth and yield research as part of Sonderforschungsbereich SFB 607 "Growth and Parasite Defense" and for funding the Trans-regional Collaborative Research Center 38 (SFB/I'RR 38) "Structures and processes of the initial ecosystem development phase in an artificial water catchment" and the Bavarian State Ministry for Nutrition, Agriculture, and Forestry for permanent support of project W 07 "Long-term experimental plots for forest growth and yield research". Thanks are also due to Ulrich Kern for the graphical artwork and the anonymous reviewers for their constructive criticism Canadian science publishing, nrc research press Ottawa},
  abstract = {Current individual tree growth models rarely consider the mode of tree competition, which can be size-asymmetric when growth is limited by light or size-symmetric when belowground resources are scarce. Even with the same competition index, growth reactions may vary considerably due to a prevailing resource limitation, as the dominant trees in a stand benefit disproportionately more on light-limited sites. To scrutinize and model the relationship between mode of competition and site conditions, 34 long-term experiments with 120 plots dating back to 1871 were used The data cover the dominating tree species in central Europe along a broad range of ecological conditions For Norway spruce (Picea abies (L) Karst). Scots pine (Pinta sylvestris L.), and sessile oak (Quercus petrea (Matt) Liebl.), stronger light competition can be shown on fertile sites compared with sites with poorer conditions Based on these findings, we constructed an enhanced version of a classic potential modifier growth model. Simulations for archetypical stands yield a transition from size-asymmetric to size-symmetric competition along the gradient from fertile to poor sites that is not covered by traditional models It was concluded that by integrating the interaction between competition and site quality, individual tree models become more site sensitive, a prerequisite for their application under fluctuating environmental conditions},
  doi =      {10.1139/x09-195},
  issn =     {0045-5067},
  keywords = {plant-populations density index models spruce mortality simulator height saxony light l.},
  type =     {Journal Article},
  url =      {<Go to ISI>://WOS:000275819500017}
}

