@Article{Sardin2008,
  author =   {Sardin, T.},
  title =    {Chênaies continentales. Guide des sylvicultures},
  journal =  {Office National des Forêts (eds)},
  year =     {2008},
  pages =    {455 pages},
  abstract = {Les chênes sessiles et pédonculés sont des essences emblématiques de la forêt française et celles dont les sylvicultures ont été les plus étudiées. Ce guide s'intéresse aux chênaies continentales, le plus grand bassin de production. Avec près de 800.000 ha sur lesquels on récolte annuellement pus d'un million de mètres cubes (hors houppiers et affouage), les chênaies continentales contribuent largement à la protection des milieux et des paysages, au maintien d'une grande biodiversité et à l'accueil du public.

Cet ouvrage met à la disposition des gestionnaires des règles sylvicoles adaptées et éprouvées pour la plupart des situations rencontrées. Les nombreux itinéraires techniques proposés permettent d'adapter la gestion aux contextes stationnels, de prendre en compte la diversité des structures liées au passé sylvicole et de conduire spécifiquement chacune des deux grandes essences de chêne présentes, le Chêne sessile et le Chêne pédonculé, dont les exigences écologiques sont très différentes.

Ce guide vient compléter, après le guide de la chênaie atlantique paru en 2004, les outils des forestiers de l'ONF pour gérer durablement la majorité des chênaies françaises de production.},
  keywords = {site index Quercus forest productivity forest management forest modelling height growth autecology competition sylviculture},
  type =     {Journal Article}
}

@Article{SaulnierEdouardCoronaEtAl2011,
  author =   {Saulnier, M. and Edouard, J. L. and Corona, C. and Guibal, F.},
  title =    {Climate/growth relationships in a Pinus cembra high-elevation network in the Southern French Alps},
  journal =  {Annals of Forest Science},
  year =     {2011},
  volume =   {68},
  number =   {1},
  pages =    {189-200},
  note =     {ISI Document Delivery No.: 762BP Times Cited: 0 Cited Reference Count: 38 Saulnier, Melanie Edouard, Jean-Louis Corona, Christophe Guibal, Frederic French ANR "Vulnerabilite: milieux et climat"[ANR-06-VULN-010]; Parc Naturel Regional du Queyras and Parc National du Mercantour This work was supported by the project ESCARSEL (ANR-06-VULN-010), founded by the French ANR "Vulnerabilite: milieux et climat", and the Parc Naturel Regional du Queyras and Parc National du Mercantour. Thanks are due to V. Petitcolas and Fleur-Meijer who made, respectively, ten and one chronologies available. Precipitation and temperature data were provided by the HISTALP project. We are deeply grateful to I. Auer for providing us with the latest revised version of the dataset. We also thank two anonymous referees who improved the earlier version of this manuscript. Springer france Paris},
  abstract = {Introduction In the context of climate change, assessing climate-growth relationships is of high importance in order to understand how forest ecosystems evolve and to test climate models at regional scale. Objectives This study aims to identify the climate variables that explain most of the variation in Pinus cembra radial growth at different spatial-temporal scales, response functions and moving response functions were processed on chronologies from a dense tree ring network. This original network is centred on the French Alps influenced by the Atlantic, Continental, and Mediterranean synoptic systems. Results A spatial clustering pattern matching the latitudinal climatic gradient was observed in tree ring indexed chronologies and climate responses. The stationary response functions computed for each cluster-averaged population evidenced a thermo-dependent northern cluster (Cl1), a rainfall-dependent southern cluster (Cl3), and an alternative response for the intermediate cluster in between (Cl2). Since the late nineteenth century, the moving response functions indicate an increasing influence of winter precipitation for the northern and the intermediate clusters. Conclusion Considering the increase in temperature and reduction both of summer precipitation predicted by general circulation models and of the snow cover and duration in the French Alps, stone pine is likely to be exposed to stressful conditions during the twenty-first century.},
  doi =      {10.1007/s13595-011-0020-3},
  issn =     {1286-4560},
  keywords = {Dendrochronology French Alps Pinus cembra Climate-growth responses tree-ring growth alpine timberline larix-decidua time-series forests},
  type =     {Journal Article},
  url =      {<Go to ISI>://WOS:000290447600020}
}

