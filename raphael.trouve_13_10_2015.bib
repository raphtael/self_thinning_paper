@Article{Dreyfus2008,
  author =   {Dreyfus, P.},
  title =    {Dynamics of Silver fir, Beech and Pines in the Mediterranean mountains: from modelling to decision-making},
  journal =  {Revue Forestière Française},
  year =     {2008},
  volume =   {60},
  number =   {2},
  pages =    {233-249},
  note =     {Cited Reference Count: 12 ref. Ateliers REGEFOR 2007, Forets melangees: quels scenarios pour l'avenir? Nancy, France, 26-28 Juin 2007.},
  abstract = {In the French Mediterranean mountains, recolonization of older Pine plantations by Beech and, on northern slopes, by Silver fir began several decades ago. A forest dynamics model was built in order to represent this evolution. The processes considered are tree growth, tree mortality and recruitment of seedlings, taking into account even those seed stands located several hundreds of meters away. The model also deals with site conditions and reduction of competition by way of various kinds of cutting (including regeneration cutting, precommercial thinning). Simulations can be performed for one or a number of forest compartments or for a whole forest or landscape. The relevance of simulation results depends on the input information. In order for this model to become a decision tool to design cutting plans or for managing a whole forest, the simulation software needs to be improved in the area of implementation of silvicultural rules and, moreover, stand description methods must be refined, joint efforts involving both scientists and forest managers are therefore needed.},
