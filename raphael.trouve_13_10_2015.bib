@Book{SchulzeTurnerGollanEtAl1987,
  title =     {Stomatal responses to air humidity and to soil drought},
  publisher = {Stanford University Press},
  year =      {1987},
  author =    {Schulze, E. D. and Turner, N. C. and Gollan, T. and Shackel, K. A.},
  series =    {Stomatal function},
  address =   {Stanford, California,},
  note =      {Cited Reference Count: 19 ref. Book chapter},
  abstract =  {Experimental evidence was given that stomata respond to changes in RH and soil water status, but there was no response in relation to leaf water status in the range of water potentials that could be obtained by perturbations via changes of the tranpiration rate. Stomata responded to soil water status in such a way that the decrease in soil water status was not mediated by a decrease in xylem water potential. Changes in CO 2 assimilation were observed during stomatal responses to RH changes and seemed to be a consequence of rather than a controlling factor for stomatal closure. There was an aftereffect of low RH on CO 2 assimilation. Stomata also closed in response to dry air in isolated epidermes. At the cellular level, water potential gradients within the leaf caused epidermal cell turgor to be very sensitive to changes in RH. Stomata were triggered by epidermal turgor but had their own metabolic time constant and response of opening and closing. Besides a feedforward response of stomata to RH there seemed to be an additional feedforward reaction to soil drying. Evidence was presented for a direct response of stomata to dry soil for sunflowers and wheat.},
  isbn =      {0-8047-1347-2},
  keywords =  {FF062Plant-Water Relations FF060Plant Physiology and Biochemistry JJ300Soil Physics FF030Plant Morphology and Structure air temperature crops drought environment leaves Meteorology relative humidity Soil water regimes stomata Stomatal movement Sunflowers water stress Wheat Helianthus Helianthus annuus Triticum Asteraceae Asterales dicotyledons angiosperms Spermatophyta plants eukaryotes Poaceae Cyperales monocotyledons Helianthus},
  pages =     {311-321},
  type =      {Book},
