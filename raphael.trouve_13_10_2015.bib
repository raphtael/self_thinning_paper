@Article{ZhangWilmking2010,
  author =   {Zhang, Y. X. and Wilmking, M.},
  title =    {Divergent growth responses and increasing temperature limitation of Qinghai spruce growth along an elevation gradient at the northeast Tibet Plateau},
  journal =  {Forest Ecology and Management},
  year =     {2010},
  volume =   {260},
  number =   {6},
  pages =    {1076-1082},
  note =     {ISI Document Delivery No.: 647MY Times Cited: 1 Cited Reference Count: 65 Zhang, Yongxiang Wilmking, Martin DFG[WI2680/2-1]; National Basic Research Program of China[2005CB422002] We thank Prof. Xuemei Shao for her support and informative discussions, Dr. Gabriela Antunez de Mayolo Wilmking for her kind English editing, Tianyu Li, Dr. Yonghong Zheng and Yan Li for field assistance. This work was supported in parts by DFG WI2680/2-1 and National Basic Research Program of China (Grant No. 2005CB422002). We gratefully thank Dr. Peter Brown for his valuable comments that helped to improve and to clarify our manuscript. Elsevier science bv Amsterdam},
  abstract = {Divergent responses between tree growth and climate factors have been widely reported at high latitudes in the northern hemisphere. Here we show variable climate-growth relationships and divergent growth responses of Qinghai spruce (Picea crassfolia) along an elevation gradient at a mid-latitude site at the northeastern Tibetan Plateau, China. Trees from higher elevations, limited mainly by temperature, show divergent growth trends over time and two responses to climate. Some trees show increasing positive and some increasing negative responses to growing season temperature during the last decades. Trees from lower treeline show a strengthening drought stress signal over time and no divergent growth trends within sites. Our results indicate that single tree analysis might be a worthwhile tool to (1) uncover spatial-temporal changes in climate-growth relationships of trees, (2) better understand future growth performance and (3) help overcome current limitations of tree ring based climatic reconstructions. (C) 2010 Elsevier B.V. All rights reserved.},
  doi =      {10.1016/j.foreco.2010.06.034},
  issn =     {0378-1127},
  keywords = {Dendroecology Treeline Tibetan Plateau Growth response divergence tree-growth radial growth climate reconstruction precipitation rings variability sensitivity kinetics province},
  type =     {Journal Article},
  url =      {<Go to ISI>://WOS:000281623800015}
}

@Article{ZhangZhangDuan2015,
  author =    {Zhang, Xiongqing AND Zhang, Jianguo AND Duan, Aiguo},
  title =     {A Hierarchical Bayesian Model to Predict Self-Thinning Line for Chinese Fir in Southern China},
  journal =   {PLoS ONE},
  year =      {2015},
  volume =    {10},
  number =    {10},
  pages =     {1-11},
  month =     {10},
  abstract =  {<p>Self-thinning is a dynamic equilibrium between forest growth and mortality at full site occupancy. Parameters of the self-thinning lines are often confounded by differences across various stand and site conditions. For overcoming the problem of hierarchical and repeated measures, we used hierarchical Bayesian method to estimate the self-thinning line. The results showed that the self-thinning line for Chinese fir (<italic>Cunninghamia lanceolata</italic> (Lamb.)Hook.) plantations was not sensitive to the initial planting density. The uncertainty of model predictions was mostly due to within-subject variability. The simulation precision of hierarchical Bayesian method was better than that of stochastic frontier function (SFF). Hierarchical Bayesian method provided a reasonable explanation of the impact of other variables (site quality, soil type, aspect, etc.) on self-thinning line, which gave us the posterior distribution of parameters of self-thinning line. The research of self-thinning relationship could be benefit from the use of hierarchical Bayesian method.</p>},
  doi =       {10.1371/journal.pone.0139788},
  publisher = {Public Library of Science},
  url =       {http://dx.doi.org/10.1371%2Fjournal.pone.0139788}
}

