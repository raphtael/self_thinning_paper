@Article{BruechertGardiner2006,
  author =   {Brüchert, Franka and Gardiner, Barry},
  title =    {The effect of wind exposure on the tree aerial architecture and biomechanics of Sitka spruce (Picea sitchensis, Pinaceae)},
  journal =  {American Journal of Botany},
  year =     {2006},
  volume =   {93},
  number =   {10},
  pages =    {1512-1521},
  abstract = {This paper reports on the effect of wind loading below damaging strength on tree mechanical and physical properties. In a wind-exposed Sitka spruce stand in western Scotland, 60 trees at four different levels of wind exposure (10 m, 30 m, 50 m, 90 m from edge) were characterized for stem and crown size and shape and mechanical properties, including structural Young's modulus (Estruct), natural frequency, and damping ratio. Estruct increased from the stand edge to the mid-forest, but with a large inter-tree variation. Swaying frequency and damping ratio of the trees also increased with distance from edge. Wind-exposed edge trees grew shorter, but more tapered with an overall lower Estruct, allowing for greater flexural stiffness at the stem base due to the larger diameter and for higher flexibility in the crown region of the stem. The trees at the middle of the stand compensated for their increased slenderness with a higher Estruct. Thus, for the different requirements for wind-firmness at stand edge and mid-forest, an adapted combination of tree form and mechanical properties allows the best withstanding of wind loads. The results show the requirement to understand the different strategies of trees to adapt to environmental constraints and the heterogeneity of their growth reactions in response to these strategies.},
  doi =      {10.3732/ajb.93.10.1512},
  type =     {Journal Article},
  url =      {http://www.amjbot.org/content/93/10/1512.abstract}
}

