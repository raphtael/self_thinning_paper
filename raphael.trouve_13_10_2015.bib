@Article{BalandierMarquierPerretEtAl2010,
  author =   {Balandier, P. and Marquier, A. and Perret, S. and Collet, C. and Courbaud, B.},
  title =    {How to estimate the light in the underwood layer of the forest stand starting from the dendrometric characteristics of the population? Comment estimer la lumiere dans le sous-bois forestier a partir des caracteristiques dendrometriques des peuplements?},
  journal =  {RenDez-Vous Techniques},
  year =     {2010},
  number =   {27-28},
  pages =    {52-58},
  note =     {Cited Reference Count: 11 ref.},
  abstract = {Light drives many forest processes and silvicultural strategies but it is not easily measurable on the ground within the forest. However, the relations between light transmittance through the canopy and the dendrometric characteristics of the population are an important aid in defining different development strategies. This article discusses light transmittance by the canopy (1) in uniform (regular or even-aged) forests, where the distribution of light on the ground below the canopy is easily determined from the dendrometric characteristics of the population, and (2) in irregular (uneven-aged) and/or mixed forests, where the spatial variability of light transmittance is greatly increased. Ways of predicting light transmittance in both situations are described. Practical models (including a marteloscope) are presented for simulating the distribution of light under the canopy of a heterogeneous forest, with future projections under various thinning and growth scenarios.},
  issn =     {1763-6442},
  keywords = {KK100Forests and Forest Trees (Biology and Ecology) KK110Silviculture and Forest Management PP720Biological Resources (Plant) PP500Meteorology and Climate ZZ900Techniques and Methodology canopy canopy gaps estimation forest management forests growth increment light penetration light transmission measurement mixed forests natural regeneration silviculture simulation models stand development stand structure thinning},
  type =     {Journal Article},
  url =      {<Go to ISI>://CABI:20103124410}
}

