@Article{LiangZhouVerbylaEtAl2011,
  author =   {Liang, J. J. and Zhou, M. and Verbyla, D. L. and Zhang, L. J. and Springsteen, A. L. and Malone, T.},
  title =    {Mapping forest dynamics under climate change: A matrix model},
  journal =  {Forest Ecology and Management},
  year =     {2011},
  volume =   {262},
  number =   {12},
  pages =    {2250-2262},
  note =     {ISI Document Delivery No.: 858DG Times Cited: 0 Cited Reference Count: 62 Liang, Jingjing Zhou, Mo Verbyla, Dave L. Zhang, Lianjun Springsteen, Anna L. Malone, Thomas Division of Forestry and Natural Resources, West Virginia University; United States Department of Agriculture[ALK-03-12]; School of Natural Resources and Agricultural Sciences, University of Alaska Fairbanks We thank Dr. Pieter Zuidema, Dr. Nicolas Picard, Dr. Tara M. Barrett, Dr. Joseph Buongiorno, and the anonymous reviewers for their invaluable critiques and insightful ideas. The spatial analysis was assisted by the Scenarios Network for Alaska and Arctic Planning of the University of Alaska Fairbanks. We also thank Nancy Tarnai for her editorial assistance. This research is supported in part by the Division of Forestry and Natural Resources, West Virginia University, the United States Department of Agriculture McIntire-Stennis Act Fund ALK-03-12, and the School of Natural Resources and Agricultural Sciences, University of Alaska Fairbanks. Elsevier science bv Amsterdam},
  abstract = {Global climate change may be affecting forests around the world. However, the impact of climate change on forest population dynamics, especially at the landscape or regional level, has hardly been addressed before. A new methodology was proposed to enable matrix transition models to account for climate impact on forest population dynamics. The first climate-sensitive matrix (CSMatrix) model was developed for the Alaska boreal forest based on observations from over 15 years of forest inventory. The spatially explicit model was used to map climate-induced forest population dynamics across the region. The model predicted that the basal area increment in the region under natural succession would be hindered by global warming, more so for dry upland areas than for moist wetlands. It was suggested that temperature-induced drought stress could more than offset a predicted increase of future precipitation in the region to lower overall forest productivity. At the same time, stand diversity would increase across the region through transient species redistribution. Accounting for climate conditions made the CSMatrix model more accurate than conventional matrix models. (C) 2011 Elsevier B.V. All rights reserved.},
  doi =      {10.1016/j.foreco.2011.08.017},
  issn =     {0378-1127},
  keywords = {Forest population dynamics Transition matrix model Empirical model Deterministic simulation Climate-induced change Climate scenario coupled model united-states growth-model management temperature simulation impacts alaska conservation productivity},
  type =     {Journal Article},
  url =      {<Go to ISI>://WOS:000297775500015}
}

