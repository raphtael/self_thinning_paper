@Article{WebbStokesScanlan2012,
  author =   {Webb, N. P. and Stokes, C. J. and Scanlan, J. C.},
  title =    {Interacting effects of vegetation, soils and management on the sensitivity of Australian savanna rangelands to climate change},
  journal =  {Climatic Change},
  year =     {2012},
  volume =   {112},
  number =   {3-4},
  pages =    {925-943},
  note =     {ISI Document Delivery No.: 943FJ Times Cited: 0 Cited Reference Count: 60 Webb, Nicholas P. Stokes, Chris J. Scanlan, Joe C. Australian Department of Agriculture, Fisheries and Forestry (DAFF) We acknowledge the support of Greg McKeon and Giselle Whish in providing access to the GRASP model and model parameterisation files. We thank Mark Howden, Andrew Moore and the anonymous reviewers for their comments which have significantly improved the quality of the manuscript. This work was supported in part by the Australian Department of Agriculture, Fisheries and Forestry (DAFF) Climate Change Research Program. Springer Dordrecht},
  abstract = {There is an increasing need to understand what makes vegetation at some locations more sensitive to climate change than others. For savanna rangelands, this requires building knowledge of how forage production in different land types will respond to climate change, and identifying how location-specific land type characteristics, climate and land management control the magnitude and direction of its responses to change. Here, a simulation analysis is used to explore how forage production in 14 land types of the north-eastern Australian rangelands responds to three climate change scenarios of +3A degrees C, +17% rainfall; +2A degrees C, -7% rainfall; and +3A degrees C, -46% rainfall. Our results demonstrate that the controls on forage production responses are complex, with functional characteristics of land types interacting to determine the magnitude and direction of change. Forage production may increase by up to 60% or decrease by up to 90% in response to the extreme scenarios of change. The magnitude of these responses is dependent on whether forage production is water or nitrogen (N) limited, and how climate changes influence these limiting conditions. Forage production responds most to changes in temperature and moisture availability in land types that are water-limited, and shows the least amount of change when growth is restricted by N availability. The fertilisation effects of doubled atmospheric CO2 were found to offset declines in forage production under 2A degrees C warming and a 7% reduction in rainfall. However, rising tree densities and declining land condition are shown to reduce potential opportunities from increases in forage production and raise the sensitivity of pastures to climate-induced water stress. Knowledge of these interactions can be applied in engaging with stakeholders to identify adaptation options.},
  doi =      {10.1007/s10584-011-0236-0},
  issn =     {0165-0009},
  keywords = {elevated atmospheric co2 ecosystem model land condition change impacts global change responses grasslands queensland vulnerability agriculture},
  type =     {Journal Article},
  url =      {<Go to ISI>://WOS:000304105600019}
}

