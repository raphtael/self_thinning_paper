@Book{Saint-AndreLaclauDeleporteEtAl2008,
  title =     {Slash and litter management effects on Eucalyptus productivity: a synthesis using a growth and yield modelling approach},
  publisher = {Center for International Forestry Research (CIFOR)},
  year =      {2008},
  author =    {Saint-Andre, L. and Laclau, J. P. and Deleporte, P. and Gava, J. L. and Goncalves, J. L. M. and Mendham, D. and Nzila, J. D. and Smith, C. and Toit, B. du and Xu, D. P. and Sankaran, K. V. and Marien, J. N. and Nouvellon, Y. and Bouillet, J. P. and Ranger, J.},
  series =    {Site management and productivity in tropical plantation forests: Proceedings of workshops in Piracicaba},
  address =   {Jakarta, Indonesia},
  note =      {Cited Reference Count: 46 ref. Brazil Book chapter; Conference paper Site management and productivity in tropical plantation forests: Proceedings of workshops in Piracicaba (Brazil), 22-26 November 2004 and Bogor (Indonesia), 6-9 November 2006.},
  abstract =  {Within the context of sustainable management of forest ecosystems, a network was started by the Center for International Forestry Research in 1995 to evaluate the impacts of slash management practices on the productivity of eucalypt, acacia and pine plantations. This network has now reached one full rotation for the ten eucalypt sites in Australia, Brazil, China, Congo, India and South Africa. This paper reports an overall synthesis of tree and stand growth. Using a growth and yield modelling approach, we investigated the effects of slash management on site index defined as the asymptotic dominant height, basal area growth and stand structure. There were significant effects of slash and litter management treatments on two processes that drive tree and stand growth: site index and ability of trees to capture the site potential. These impacts were strongly site dependent. As expected, the soil organic carbon concentration could not be used alone to predict the intensity of stand response. Conversely, a ratio between nitrogen content in slash and litter prior to planting and the nitrogen concentration within the 0-10 cm soil layer, was a good predictor of the differences in stand productivity across treatments.},
  isbn =      {978-979-1412-58-2},
  keywords =  {JJ200Soil Chemistry and Mineralogy KK110Silviculture and Forest Management ZZ100Mathematics and Statistics forest litter forest management forest trees forests growth growth models nitrogen content organic carbon productivity silviculture site class assessment slash soil organic matter stand structure sustainability trees yields Australia Brazil China Congo India South Africa Eucalyptus Australasia Oceania Developed Countries Commonwealth of Nations OECD Countries South America America Developing Countries Threshold Countries Latin America East Asia Asia Central Africa Africa South of Sahara Africa ACP Countries Francophone Africa Myrtaceae Myrtales dicotyledons angiosperms Spermatophyta plants eukaryotes South Asia Southern Africa Anglophone Africa},
  pages =     {173-189},
  type =      {Book},
  url =       {<Go to ISI>://CABI:20093087607
http://www.cifor.cgiar.org/publications/pdf-files/Books/BKallio0801.pdf}
