@Article{HasenauerNemaniSchadauerEtAl1999,
  author =   {Hasenauer, H. and Nemani, R. R. and Schadauer, K. and Running, S. W.},
  title =    {Forest growth response to changing climate between 1961 and 1990 in Austria},
  journal =  {Forest Ecology and Management},
  year =     {1999},
  volume =   {122},
  number =   {3},
  pages =    {209-219},
  note =     {ISI Document Delivery No.: 227YQ Times Cited: 39 Cited Reference Count: 30 Hasenauer, H Nemani, RR Schadauer, K Running, SW Elsevier science bv Amsterdam},
  abstract = {Using 30 years of climate records from 20 weather stations, we investigate the magnitude of temperature and precipitation change, and change in the length of the growing season between 1961 and 1990. Special attention is paid to the period between 1981 and 1990, because recent research suggests that, during this time-span, forest productivity may have increased in the northern latitudes. In order to understand the importance of changes in climate on forest growth, we use the ecosystem model FOREST-BGC as a diagnostic tool to predict the annual net primary production (NPP). The results of our study indicate: no change in precipitation between 1961 and 1990; a significant (alpha = 0.05) increase in mean annual temperature of 0.72 degrees C, mean annual minimum temperature (0.80 degrees C), winter temperature (2.36 degrees C) as well as an increase in the length of the temperature controlled growing season by 11 days, resulting in a significant increase in diameter increment obtained from 1179 cores of Norway spruce across Austria. The trends in NPP are consistent with observed increment rates validating the use of biogeochemical modeling as a diagnostic tool to search for possible causes on changing environmental conditions. (C) 1999 Elsevier Science B.V. All rights reserved.},
  doi =      {10.1016/s0378-1127(99)00010-9},
  issn =     {0378-1127},
  keywords = {growth trends climate change growth and yield Norway spruce FOREST-BGC model balance rise},
  type =     {Journal Article},
