@Article{SidorovaSaurerMyglanEtAl2012,
  author =   {Sidorova, Olga V. and Saurer, Matthias and Myglan, Vladimir S. and Eichler, Anja and Schwikowski, Margit and Kirdyanov, Aleksander V. and Bryukhanova, Marina V. and Gerasimova, Oksana V. and Kalugin, Ivan A. and Daryin, Andrey V. and Siegwolf, Rolf T. W.},
  title =    {A multi-proxy approach for revealing recent climatic changes in the Russian Altai},
  journal =  {Climate Dynamics},
  year =     {2012},
  volume =   {38},
  number =   {1-2},
  pages =    {175-188},
  note =     {Times Cited: 1},
  abstract = {For the first time we present a multi-proxy data set for the Russian Altai, consisting of Siberian larch tree-ring width (TRW), latewood density (MXD), delta(13)C and delta(18)O in cellulose chronologies obtained for the period 1779-2007 and cell wall thickness (CWT) for 1900-2008. All of these parameters agree well between each other in the high-frequency variability, while the low-frequency climate information shows systematic differences. The correlation analysis with temperature and precipitation data from the closest weather station and gridded data revealed that annual TRW, MXD, CWT, and delta(13)C data contain a strong summer temperature signal, while delta(18)O in cellulose represents a mixed summer and winter temperature and precipitation signal. The temperature and precipitation reconstructions from the Belukha ice core and Teletskoe lake sediments were used to investigate the correspondence of different independent proxies. Low frequency patterns in TRW and delta(13)C chronologies are consistent with temperature reconstructions from nearby Belukha ice core and Teletskoe lake sediments showing a pronounced warming trend in the last century. Their combination could be used for the regional temperature reconstruction. The long-term delta(18)O trend agrees with the precipitation reconstruction from the Teletskoe lake sediment indicating more humid conditions during the twentieth century. Therefore, these two proxies could be combined for the precipitation reconstruction.},
  doi =      {10.1007/s00382-010-0989-6},
  issn =     {0930-7575},
  type =     {Journal Article},
  url =      {<Go to ISI>://WOS:000298753200011}
}

