@Article{Bunn2008,
  author =   {Bunn, A. G.},
  title =    {A dendrochronology program library in R (dplR)},
  journal =  {Dendrochronologia},
  year =     {2008},
  volume =   {26},
  number =   {2},
  pages =    {115-124},
  note =     {ISI Document Delivery No.: 360FU Times Cited: 21 Cited Reference Count: 18 Bunn, Andrew G. Western Washington University and the National Science Foundation[ARC-0612346, ATM-0629172] I would like to acknowledge support from the Office of Research and Sponsored Programs at Western Washington University and the National Science Foundation (ARC-0612346 and ATM-0629172). A. Lloyd and G. Pederson provided helpful tests of dplR and suggestions from two anonymous reviewers greatly improved the quality of the manuscript. I also thank all of the researchers who contribute data to the International Tree-Ring Data Bank and thus greatly advance the field of dendrochronology. Elsevier gmbh, urban \& fischer verlag Jena},
  abstract = {I present and describe a new software package in the R statistical programming environment for dendrochronology. R is considered the world's pre-eminent open-source statistical computing environment where users can contribute packages. which Lire freely available on the Internet. The dendrochronology program library in R (dplR) is able to read standard decadal-format files and allows users to perform several standard analyses including interactive detrending, chronology building. and the calculation of standard descriptive statistics. The package can also produce a variety of publication quality plots. The dplR package should make it easier for dendrochronologists to take advantage of R and use it as their primary analytic environment. (C) 2008 Elsevier GmbH. All rights reserved.},
