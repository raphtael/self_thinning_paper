@Article{Kantor2008,
  author =   {Kantor, P.},
  title =    {Production potential of Douglas fir at mesotrophic sites of Krtiny Training Forest Enterprise},
  journal =  {Journal of Forest Science},
  year =     {2008},
  volume =   {54},
  number =   {7},
  pages =    {321-332},
  abstract = {This study evaluated the production parameters (height, diameter at breast height, volume) of mature Douglas fir ( Pseudotsuga menziesii) at mesotrophic sites of the Krtiny Training Forest Enterprise in the Czech Republic. A total of 29 mixed stands were assessed with the registered proportion of Douglas fir at the age of 85-136 years. Comparing the 10 largest Douglas firs with the 10 largest spruces or larches, and as a rule markedly higher, the production potential of introduced Douglas fir was found in all assessed stands. There were also groups of trees where the volume of Douglas fir was twice to 3 times higher than the volume of spruce or larch. For example, in stand 177B11, the mean volume of 9.12 m 3 was recorded in the 10 largest Douglas fir trees but the volume of spruce reached only 3.17 m 3 and the volume of larch was 3.70 m 3. Differences in mensurational parameters of Douglas fir, found on the one hand, and of Norway spruce ( Picea abies) or European larch ( Larix decidua), on the other hand, compared by ANOVA tests were statistically highly significant. Annual ring analyses have shown that at present the volume increment of particular Douglas fir trees ranges from 0.12 to 0.16 m 3 year -1 in mature stands (i.e. ~1.5 m 3 every 10 years).},
  issn =     {1212-4834},
  type =     {Journal Article},
  url =      {<Go to ISI>://CABI:20083216932}
}

