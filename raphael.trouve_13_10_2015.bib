@InCollection{Westoby1984,
  author =    {Mark Westoby},
  title =     {The Self-Thinning Rule },
  publisher = {Academic Press},
  year =      {1984},
  editor =    {A. MacFadyen and E.D. Ford},
  volume =    {14},
  series =    {Advances in Ecological Research },
  pages =     {167 - 225},
  abstract =  {Publisher Summary The self-thinning rule describes plant mortality because of competition in crowded even-aged stands. The rule is best understood with respect to a graph of log biomass (log B) per unit area vs. log density (log N) of survivors, known as the B–N diagram. The rule has three notable features—(1) mortality is a function only of biomass accumulation, (2) because mortality is driven by the rate of accumulation of biomass, mortality is slower when conditions for growth are worse, and (3) the thinning line has a slope of about –½ for most studied species under most conditions. Two main effects operate in developing distributions in even-aged stands. First, large plants suppress small plants. The result is a “hierarchy of dominance and suppression” in which the smaller plants are at an accumulating disadvantage and finally die. Second, the mortality of smaller individuals truncates the distribution from the left. },
  doi =       {http://dx.doi.org/10.1016/S0065-2504(08)60171-3},
  issn =      {0065-2504},
  url =       {http://www.sciencedirect.com/science/article/pii/S0065250408601713}
}

@Article{WhiteGevelGrissino-MayerEtAl2011,
  author =   {White, P. B. and van de Gevel, S. L. and Grissino-Mayer, H. D. and Laforest, L. B. and Deweese, G. G.},
  title =    {Climatic response of oak species across an environmental gradient in the southerb appalachian mountain, USA},
  journal =  {Tree-Ring Research},
  year =     {2011},
  volume =   {67},
  number =   {1},
  pages =    {27-37},
  note =     {ISI Document Delivery No.: 707RX Times Cited: 0 Cited Reference Count: 58 White, Philip B. van de Gevel, Saskia L. Grissino-Mayer, Henri D. Laforest, Lisa B. Deweese, Georgina G. Jefferson National Forest; Great Smoky Mountains National Park; Joint Fire Science Program We thank the Jefferson National Forest, Great Smoky Mountains National Park, and the Joint Fire Science Program for providing funding and logistical support and permitting access to our research areas. We would also like to thank Charles Lafon, Rob Klein, Jessica D. Slayton, Jim R. LaForest, Jr., Marian LaForest, Maggie Stevens, Travis Kilgore, and Eric Miller for their help with data collection and analyses. Tree-ring soc Tucson},
  abstract = {We investigated the climatic sensitivity of oak species across a wide elevation range in the southern Appalachian Mountains, an area where greater knowledge of oak sensitivity is desired. We developed three tree-ring chronologies for climatic analyses from oak cores taken from the Jefferson National Forest, Virginia, and Great Smoky Mountains National Park, Tennessee. We statistically compared the three chronologies with monthly climatic data from 1930 to 2005. The results of our analyses suggest that oak species in the southern Appalachian Mountains require a cool, moist summer for above average-growth to occur. The climate signal increased in duration from high to low elevational and latitudinal gradients, indicating a strong moisture-preconditioning signal during the previous fall at our lowest elevation site. A notable finding of this research was the degree of responsiveness in oaks that are growing in forest interior locations where strong climate sensitivity would not be expected because of the effects of internal stand dynamics. Furthermore, the relationships between evapotranspiration rates and the geographic factors of elevation, latitude, and aspect influence the climate signals at the three sites. Our research suggests that oaks located in a warm and xeric climate experience more physiological stress and put forth a more varied climatic response.},
  doi =      {10.3959/2009-1.1},
  issn =     {1536-1098},
  keywords = {Quercus dendroclimatology dendrochronology tree rings climate response southern Appalachian Mountains southeastern united-states national-park tree-rings southwestern virginia forest decline radial growth red spruce white oak drought fire},
  type =     {Journal Article},
  url =      {<Go to ISI>://WOS:000286304800003}
}

@Article{WhiteheadEdwardsJarvis1984,
  author =   {Whitehead, D. and Edwards, W. R. N. and Jarvis, P. G.},
  title =    {Conducting sapwood area, foliage area, and permeability in mature trees of Piceasitchensis and Pinuscontorta},
  journal =  {Canadian Journal of Forest Research},
  year =     {1984},
  volume =   {14},
  number =   {6},
  pages =    {940-947},
  abstract = {Au moyen d'une analyse physiologique basée sur la loi de Darcy, les auteurs ont étudié les relations liant la surface foliaire et la surface d'aubier entre les arbres et à l'intérieur des cimes de 20 Piceasitchensis (Bong.) Carr. provenant des îles Queen Charlotte, C.-B. (10 arbres d'une parcelle témoin et 10 arbres d'une parcelle fertilisée 8 ans plut tôt avec du potassium et du phosphore) et 10 Pinuscontorta Dougl. (provenance Ladysmith). L'indice de surface foliaire dans la parcelle fertilisée de P. sitchensis était plus élevé que celui de la parcelle témoin. La variation de la densité du feuillage à travers la cime suivait une distribution normale. Les relations entre la surface foliaire et la surface terrière d'aubier étaient linéaires, mais les pentes étaient différentes pour les deux espèces. Il n'y avait pas de différence significative entre les arbres témoins et ceux fertilisés de P. sitchensis. La relation entre la surface foliaire et le produit surface d'aubier × perméabilité était linéaire et les données des trois parcelles se groupaient sur la même droite. La surface d'aubier, la perméabilité et leur produit diminuaient avec la profondeur à travers la cime des arbres. À l'intérieur des cimes, les relations entre la surface foliaire cumulative et la surface d'aubier, et entre la surface foliaire cumulative et le produit surface d'aubier × perméabilité, différaient selon les espèces et les traitements. En pointant graphiquement le produit de la surface foliaire cumulative au-dessus d'un entre-noeud × la longueur internodale avec le produit de la surface d'aubier × la perméabilité pour l'entre-noeud, on a obtenu une seule relation linéaire. Cela suggère que c'est la chute de potentiel à travers un noeud et un entre-noeud, plutôt que le gradient de potentiel à travers l'entre-noeud, qui est reliée au flux de l'eau à travers la cime des arbres. Les données supportent l'hypothèse que la relation entre la surface foliaire et la surface d'aubier dépend de la perméabilité de l'aubier et du climat local, à travers son influence sur le taux de transpiration, en particulier via le déficit moyen de pression de vapeur d'eau de l'air et la conductance stomatale.},
  doi =      {doi:10.1139/x84-166},
  type =     {Journal Article},
  url =      {http://www.nrcresearchpress.com/doi/abs/10.1139/x84-166}
}

